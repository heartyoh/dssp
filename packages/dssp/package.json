{
  "name": "@dssp/dssp",
  "version": "0.0.1",
  "main": "dist-server/index.js",
  "browser": "dist-client/index.js",
  "things-factory": true,
  "author": "heartyoh <heartyoh@hatiolab.com>",
  "description": "Revolutionizing Safety Oversight: The Future of Digital Supervision",
  "publishConfig": {
    "access": "public",
    "@dssp:registry": "https://registry.npmjs.org"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hatiolab/dssp.git",
    "directory": "packages/dssp"
  },
  "scripts": {
    "serve": "things-factory dssp",
    "serve:dev": "npm run build:server && concurrently -k -r \"npm run build:client:watch\" \"things-factory-dev dssp\"",
    "copy:files": "copyfiles -e \"./client/**/*.{ts,js,json}\" -u 1 \"./client/**/*\" dist-client",
    "build": "npm run build:server && npm run build:client",
    "build:client": "npm run clean:client && npm run copy:files && tsc --p ./client/tsconfig.json",
    "build:client:watch": "npm run copy:files && tsc --p ./client/tsconfig.json --watch --preserveWatchOutput",
    "build:server": "npm run clean:server && tsc --p ./server/tsconfig.json",
    "build:app": "npm run clean:app && webpack --config ../../node_modules/@things-factory/builder/webpack.config.js",
    "clean:server": "rimraf dist-server",
    "clean:client": "rimraf dist-client",
    "clean:app": "rimraf dist-app",
    "clean": "npm run clean:server && npm run clean:client",
    "migration": "npm run build:server && things-factory-migration",
    "migration:privileges": "npm run build:server && things-factory-migration-privileges",
    "migration:create": "node ../../node_modules/typeorm/cli.js migration:create -d ./server/migrations",
    "migration:generate": "node ../../node_modules/typeorm/cli.js migration:generate",
    "migration:run": "node ../../node_modules/typeorm/cli.js migration:run",
    "migration:revert": "node ../../node_modules/typeorm/cli.js migration:revert",
    "stop:dev": "kill $(lsof -t -i:3000,3001)",
    "stop": "things-factory-stop dssp",
    "docker": "npm run build:app && things-factory-dockerize hatiolab/dssp:latest  hatiolab/dssp:$npm_package_version linux/amd64",
    "docker:run": "docker run --platform linux/amd64 -p 4000:3000 hatiolab/dssp:latest",
    "docker:push": "docker image push hatiolab/dssp:latest && docker image push hatiolab/dssp:$npm_package_version"
  },
  "dependencies": {
    "@dssp/building-complex": "^0.0.1",
    "@dssp/project": "^0.0.1",
    "@material/web": "1.5.0",
    "@operato/scene-auth": "^7.0.0-rc",
    "@operato/scene-chartjs": "^7.0.0-rc",
    "@operato/scene-clock": "^7.0.0-rc",
    "@operato/scene-data-transform": "^7.0.0-rc",
    "@operato/scene-form": "^7.0.0-rc",
    "@operato/scene-gauge": "^7.0.0-rc",
    "@operato/scene-grist": "^7.0.0-rc",
    "@operato/scene-half-roundrect": "^7.0.0-rc",
    "@operato/scene-image-slider": "^7.0.0-rc",
    "@operato/scene-integration": "^7.0.0-rc",
    "@operato/scene-legend": "^7.0.0-rc",
    "@operato/scene-news-ticker": "^7.0.0-rc",
    "@operato/scene-progressbar": "^7.0.0-rc",
    "@operato/scene-random": "^7.0.0-rc",
    "@operato/scene-switch": "^7.0.0-rc",
    "@operato/scene-tab": "^7.0.0-rc",
    "@operato/scene-table": "^7.0.0-rc",
    "@operato/scene-timer": "^7.0.0-rc",
    "@things-factory/api": "^7.0.1-rc",
    "@things-factory/apptool-base": "^7.0.1-rc",
    "@things-factory/apptool-ui": "^7.0.1-rc",
    "@things-factory/auth-base": "^7.0.1-rc",
    "@things-factory/auth-ui": "^7.0.1-rc",
    "@things-factory/board-service": "^7.0.1-rc",
    "@things-factory/board-ui": "^7.0.1-rc",
    "@things-factory/code-ui": "^7.0.1-rc",
    "@things-factory/context-ui": "^7.0.1-rc",
    "@things-factory/dashboard": "^7.0.1-rc",
    "@things-factory/export-ui": "^7.0.1-rc",
    "@things-factory/help": "^7.0.1-rc",
    "@things-factory/i18n-base": "^7.0.1-rc",
    "@things-factory/integration-base": "^7.0.1-rc",
    "@things-factory/integration-ui": "^7.0.1-rc",
    "@things-factory/lite-menu": "^7.0.1-rc",
    "@things-factory/more-base": "^7.0.1-rc",
    "@things-factory/more-ui": "^7.0.1-rc",
    "@things-factory/notification": "^7.0.1-rc",
    "@things-factory/resource-ui": "^7.0.1-rc",
    "@things-factory/setting-base": "^7.0.1-rc",
    "@things-factory/setting-ui": "^7.0.1-rc",
    "@things-factory/shell": "^7.0.1-rc",
    "@things-factory/system": "^7.0.1-rc"
  },
  "devDependencies": {
    "@things-factory/builder": "^7.0.1-rc"
  }
}
